package com.sound{		import flash.display.MovieClip;	import flash.events.Event;	import flash.media.SoundMixer;	import flash.utils.ByteArray;
	public class Subwooffer extends SubwoofferAsset {		private var _bytes:ByteArray = new ByteArray();		private var _fft:Boolean = false;		public function Subwooffer() {			//trace("fg trbdyr5n41th1n56th16n 5t14h65");			this.addEventListener(Event.ENTER_FRAME, onVisualize,								  false, 0, true);		}		private function onVisualize(evt:Event):void {			SoundMixer.computeSpectrum(_bytes, _fft);			var scaleByBytes:Number;			scaleByBytes = _bytes.readFloat()*5;			if(scaleByBytes < .2){				scaleByBytes = .2;			}			if(scaleByBytes > 1){				scaleByBytes = 1;			}			//trace(scaleByBytes);								this.scaleX = this.scaleY = scaleByBytes;		}	}}